# Ackermann function (http://rosettacode.org/wiki/Ackermann_function)

fn ackermann(m, n: int) int {
    if m == 0 {
        n + 1

    } else if n == 0 {
        ackermann(m - 1, 1)

    } else {
        ackermann(m - 1, ackermann(m, n - 1))
    }
}

fn main() int {
	for m := 0, m <= 4, m += 1 {
		for n := 0, n <= 6, n += 1 {
			println("A({}, {}) = {}", m, n, ackermann(m, n));
		}
	}

	0
}
